{
	"compilerOptions": {
		"incremental": true,
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"types": ["@cloudflare/workers-types"],
		"lib": ["ESNext"],
		"noEmit": true,
		"jsxFactory": "h",
		"jsx": "react",
		"sourceMap": true
	}
}
